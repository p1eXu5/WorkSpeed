<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:rs="clr-namespace:WorkSpeed.DesktopClient.ViewModels.ReportService">

    <DataTemplate DataType="{x:Type rs:FilterItemViewModel}">
        <CheckBox Height="22" 
                  Content="{Binding Caption}"
                  IsChecked="{Binding IsChecked}"
                  />
    </DataTemplate>

    <DataTemplate DataType="{x:Type rs:EntityFilterViewModel}">
        <ListBox ItemsSource="{Binding FilterItemVmCollection}" />
    </DataTemplate>

</ResourceDictionary>