<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:reportService="clr-namespace:WorkSpeed.DesktopClient.ViewModels.ReportService">
    
    <DataTemplate DataType="{x:Type reportService:FilterItemViewModel}">
        <CheckBox Height="22" 
                  Content="{Binding Name}"
                  IsChecked="{Binding IsChecked}"
                  Command="{Binding FilterCommand}"
                  />
    </DataTemplate>

    <DataTemplate DataType="{x:Type reportService:FilterElementViewModel}">
        <ListBox ItemsSource="{Binding FilterItemVm}" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type reportService:EntityFilterViewModel}">
        <ListBox ItemsSource="{Binding FilterElementVmCollection}" />
    </DataTemplate>

</ResourceDictionary>