<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vp="clr-namespace:WorkSpeed.DesktopClient.Views.Productivity"
                    xmlns:entities="clr-namespace:WorkSpeed.DesktopClient.Views.Entities"
                    xmlns:vme="clr-namespace:WorkSpeed.DesktopClient.ViewModels.Entities"
                    xmlns:vmp="clr-namespace:WorkSpeed.DesktopClient.ViewModels.Productivity">
    
    <DataTemplate DataType="{x:Type vmp:ProductivityViewModel}">
        <vp:ProductivityView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmp:EmployeeProductivityViewModel}">
        <vp:EmployeeProductivityView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vme:OperationViewModel}">
        <entities:OperationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vme:EmployeeViewModel}">
        <entities:EmployeeView />
    </DataTemplate>


</ResourceDictionary>