<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

                    xmlns:vp="clr-namespace:WorkSpeed.DesktopClient.Views.ReportService.Productivity"
                    xmlns:ve="clr-namespace:WorkSpeed.DesktopClient.Views.ReportService.Entities"

                    xmlns:vmp="clr-namespace:WorkSpeed.DesktopClient.ViewModels.ReportService.Productivity"
                    xmlns:vme="clr-namespace:WorkSpeed.DesktopClient.ViewModels.ReportService.Entities"
                    >

    <DataTemplate DataType="{x:Type vme:OperationViewModel}">
        <vp:OperationHeaderView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmp:ProductivityViewModel}">
        <vp:ProductivityView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmp:EmployeeProductivityViewModel}">
        <vp:EmployeeProductivityView />
    </DataTemplate>

</ResourceDictionary>