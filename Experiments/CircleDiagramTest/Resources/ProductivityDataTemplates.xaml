<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:CircleDiagramTest.ViewModels"
                    xmlns:vp="clr-namespace:CircleDiagramTest.Views.Productivity"
                    xmlns:ve="clr-namespace:CircleDiagramTest.Views.Entities">
    
    <DataTemplate DataType="{x:Type viewModels:ProductivityViewModel}">
        <vp:ProductivityView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:EmployeeProductivityViewModel}">
        <vp:EmployeeProductivityView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:OperationViewModel}">
        <ve:OperationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:EmployeeViewModel}">
        <ve:EmployeeView />
    </DataTemplate>


</ResourceDictionary>